<h1>Listing computation_units</h1>

<table>
  <thead>
    <tr>
      <th>Ram</th>
      <th>Cpu cores</th>
      <th>Cpu clock</th>
      <th>Storage</th>
      <th>Nic</th>
      <th>Online</th>
      <th>Name</th>
      <th>Location</th>
      <th>Added</th>
      <th>Updated</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @computation_units.each do |computation_unit| %>
      <tr>
        <td><%= computation_unit.ram %></td>
        <td><%= computation_unit.CPU_Cores %></td>
        <td><%= computation_unit.CPU_Clock %></td>
        <td><%= computation_unit.storage %></td>
        <td><%= computation_unit.ip %></td>
        <td><%= computation_unit.uplink %></td>
        <td><%= computation_unit.downlink %></td>
        <td><%= computation_unit.online %></td>
        <td><%= computation_unit.name %></td>
        <td><%= computation_unit.location %></td>
        <td><%= computation_unit.added %></td>
        <td><%= computation_unit.updated %></td>
        <td><%= link_to 'Show', computation_unit %></td>
        <td><%= link_to 'Edit', edit_computation_unit_path(computation_unit) %></td>
        <td><%= link_to 'Destroy', computation_unit, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Computation unit', new_computation_unit_path %>
